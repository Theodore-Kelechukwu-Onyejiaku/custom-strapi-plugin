    },
    "canonicalUrl": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "banner": {
      "type": "string"
    },
    "post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::content-publisher.post",
      "mappedBy": "blog"
    }
  }
